{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
    "outDir": "./app",
    "baseUrl": "./",
    "strict": false,
    "skipLibCheck": true,
    "sourceMap": true,
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "*test*", "*.txt"]
}
